        <class_name>Processor</class_name>
        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r = getRow();
  
  if (r == null) {
    setOutputDone();
    return false;
  }
  
  Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
  
  String json = get(Fields.In, "json").getString(r);
  String componentes = get(Fields.In, "componentes").getString(r);
  
  if (componentes.equals("[]") == false &amp;&amp; componentes.equals("{}") == false) {
    String json_aux = "\"componentes\":[" + componentes + "]";
    String json_aux2 = json;
    String json_aux3 = json_aux2.replace("\"componentes\":[]", json_aux);
    get(Fields.Out, "json").setValue(outputRow, json_aux3);
  }
  
  putRow(data.outputRowMeta, outputRow);
  
  return true;
}
</class_source>
