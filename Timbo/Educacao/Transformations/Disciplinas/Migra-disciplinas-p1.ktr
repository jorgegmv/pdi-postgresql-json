    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r = getRow();
  
  if (r == null) {
    setOutputDone();
    return false;
  }
  
  Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
  
  String ca2_nome = get(Fields.In, "ca2_nome").getString(r);
  String ca2_sigla = get(Fields.In, "ca2_sigla").getString(r);
  String descricao_novo = get(Fields.In, "descricao_novo").getString(r);
  String sigla_nova = get(Fields.In, "sigla_nova").getString(r);
  
  if (descricao_novo != null &amp;&amp; descricao_novo.length() > 0) {
    String[] descricao = descricao_novo.replaceAll(" E ", " ").replaceAll(" DE ", " ").split(" ");
    String sigla = "";
    
    if (descricao.length == 1 &amp;&amp; descricao[0].indexOf("/") == -1) {
      if (descricao[0].trim().length() > 5) {
        sigla = descricao[0].trim().substring(0, 5);
      }
      else {
        sigla = descricao[0].trim();
      }
    }
    else {
      for (int i = 0; i &lt; descricao.length; i++) {
        if (descricao[i].trim().length() > 1) {
          if (descricao[i].indexOf("/") == -1) {
            sigla = sigla + descricao[i].trim().substring(0, 2);
          }
          else {
            String[] aux = descricao[i].trim().split("/");
            if (aux.length > 1) {
              if (aux[1].trim() != null &amp;&amp; aux[1].trim().length() > 0) {
                if (aux[1].trim().length() > 1) {
                  sigla = sigla + aux[0].trim().substring(0, 1) + aux[1].trim().substring(0, 1);
                }
                else {
                  sigla = sigla + aux[0].trim().substring(0, 2) + aux[1].trim();
                }
              }
              else {
                sigla = sigla + aux[0].trim().substring(0, 2);
              }
            }
            else {
              sigla = sigla + descricao[i].trim();
            }
          }
        }
        else {
          sigla = sigla + descricao[i].trim();
        }
      }
    }
    if (sigla.length() > 5) {
      get(Fields.Out, "sigla_nova").setValue(outputRow, sigla.substring(0, 5));
    }
    else {
      get(Fields.Out, "sigla_nova").setValue(outputRow, sigla);
    }
  }
  
  putRow(data.outputRowMeta, outputRow);
  
  return true;
}
</class_source>
